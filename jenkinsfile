pipeline{
       agent any

     stage{
         stage("clone') {
           step{
                 echo 'cloning repository...'
       }
} 
            stage(install') {
             step{
                   sh 'npm install'
                  }
           }
                 stage('Run') {
                      step{
                            sh 'npm start'
                       }
               }
        }
}



 
          
